{
  "name": "@jurloogar/doctor_guide_api",
  "type": "commonjs",
  "version": "1.5.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Julooga/doctor_guide_api.git"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/dev.ts",
    "build:api": "PROFILE=real esbuild --bundle --outfile=./dist/index.js --platform=node --target=node20 --minify ./src/index.ts",
    "build:spec": "PROFILE=real tsx src/services/swagger/generateSwagger.ts -o './dist'",
    "build": "run-s build:api build:spec",
    "typecheck": "tsc",
    "prettier": "prettier --write \"**/*.{ts,tsx,cjs,mjs,json,html,css,js,jsx}\" --cache --config prettier.config.mjs",
    "eslint": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "format": "run-s typecheck prettier eslint"
  },
  "devDependencies": {
    "@hono/node-server": "^1.14.1",
    "@julooga/eslint": "workspace:*",
    "@julooga/prettier": "workspace:*",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "20",
    "commander": "^13.1.0",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "esbuild": "^0.21.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "npm-run-all2": "^6.2.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "zx": "^8.5.3"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^2.2.10",
    "@ai-sdk/anthropic": "^1.2.12",
    "@aws-sdk/client-bedrock-runtime": "^3.0.0",
    "@aws-sdk/client-dynamodb": "^3.803.0",
    "@aws-sdk/credential-providers": "^3.803.0",
    "@aws-sdk/lib-dynamodb": "^3.803.0",
    "@hono/swagger-ui": "^0.5.1",
    "@hono/zod-validator": "^0.5.0",
    "@julooga/sdk": "workspace:*",
    "@langchain/aws": "^0.1.10",
    "@langchain/community": "^0.3.45",
    "@langchain/core": "^0.3.57",
    "ai": "^4.3.16",
    "electrodb": "^3.4.1",
    "hono": "^4.7.8",
    "hono-openapi": "^0.4.8",
    "langchain": "^0.3.27",
    "lodash-es": "^4.17.21",
    "swagger-typescript-api": "^13.1.3",
    "zod": "^3.24.4",
    "zod-openapi": "^4.2.4"
  }
}
