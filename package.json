{
  "name": "doctor_guide_api",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "build": "esbuild --bundle --outfile=./dist/index.js --platform=node --target=node20 ./src/index.ts",
    "zip": "zip -j lambda.zip dist/index.js",
    "update:local": "aws lambda update-function-code --zip-file fileb://lambda.zip --function-name doctor_guide_api --region ap-northeast-2 --profile 166287530750_jurloogar-jaewon",
    "update:ci": "aws lambda update-function-code --zip-file fileb://lambda.zip --function-name doctor_guide_api --region ap-northeast-2",
    "deploy:lambda": "run-s build zip update:local",
    "deploy:lambda:ci": "run-s build zip update:ci",
    "prepare": "husky",
    "type:check": "tsc --skipLibCheck --noEmit --incremental --tsBuildInfoFile ./node_modules/.cache/tsc/tsbuildinfo",
    "prettier": "prettier --write \"**/*.{ts,tsx,cjs,mjs,json,html,css,js,jsx}\" --cache --config prettier.config.mjs",
    "eslint": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "format": "run-s type:check prettier eslint"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/node": "20",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "commander": "^13.1.0",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "esbuild": "^0.21.4",
    "eslint": "^9.26.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-classnames": "^0.7.8",
    "prettier-plugin-css-order": "^2.1.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "zx": "^8.5.3"
  },
  "dependencies": {
    "hono": "^4.7.8"
  }
}
